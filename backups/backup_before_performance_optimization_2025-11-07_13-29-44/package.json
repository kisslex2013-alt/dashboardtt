{
  "name": "time-tracker",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "stop-all": "powershell -ExecutionPolicy Bypass -File ./scripts/stop-all.ps1",
    "start": "powershell -ExecutionPolicy Bypass -File ./scripts/start-single-background.ps1",
    "start:visible": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop time-tracker-dev && pm2 delete time-tracker-dev",
    "restart": "npm run stop-all && timeout /t 2 /nobreak >nul && npm run start",
    "logs": "pm2 logs time-tracker-dev",
    "status": "pm2 status time-tracker-dev",
    "pm2:start": "pm2 start npm --name \"time-tracker-dev\" -- run dev",
    "pm2:stop": "pm2 stop time-tracker-dev",
    "pm2:restart": "pm2 restart time-tracker-dev",
    "pm2:delete": "pm2 delete time-tracker-dev",
    "pm2:logs": "pm2 logs time-tracker-dev",
    "pm2:monitor": "pm2 monitor",
    "smoke-test": "node scripts/smoke-test.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@iconify/react": "^6.0.2",
    "@react-spring/web": "^10.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^2.2.2",
    "recharts": "^3.3.0",
    "tone": "^15.1.22",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "vite": "^7.1.7"
  },
  "description": "Time Tracker Dashboard - умный учет рабочего времени с аналитикой",
  "keywords": [
    "time-tracker",
    "react",
    "vite",
    "tailwindcss"
  ],
  "author": "",
  "license": "ISC"
}
