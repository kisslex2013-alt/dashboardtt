{
  "name": "time-tracker",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "node scripts/update-build-version.js && git add -A --ignore-errors && git commit -m \"update\" && git push",
    "backup": "node scripts/create-backup.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "vitest run --reporter=verbose **/*a11y*.{js,jsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test e2e/performance.spec.js",
    "test:performance:preview": "playwright test --config=playwright.performance.config.js e2e/performance.spec.js",
    "test:lighthouse": "node scripts/run-lighthouse.js",
    "test:lighthouse:ci": "lhci autorun",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:a11y": "eslint . --ext .js,.jsx --plugin jsx-a11y"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@iconify/react": "^6.0.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^2.2.2",
    "recharts": "^3.3.0",
    "tone": "^15.1.22",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@axe-core/react": "^4.10.0",
    "@eslint/js": "^9.36.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.0",
    "chrome-launcher": "^1.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-vitest": "^0.5.4",
    "form-data": "^4.0.4",
    "globals": "^16.4.0",
    "husky": "^9.0.11",
    "jsdom": "^27.1.0",
    "lighthouse": "^12.0.0",
    "lint-staged": "^15.2.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.8"
  },
  "description": "Time Tracker Dashboard - умный учет рабочего времени с аналитикой",
  "keywords": [
    "time-tracker",
    "react",
    "vite",
    "tailwindcss"
  ],
  "author": "",
  "license": "ISC"
}
